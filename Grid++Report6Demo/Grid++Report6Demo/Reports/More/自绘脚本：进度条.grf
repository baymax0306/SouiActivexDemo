		},
		{
			"Name":"Address"
		},
		{
			"Name":"City"
		},
		{
			"Name":"Region"
		},
		{
			"Name":"PostalCode"
		},
		{
			"Name":"Country"
		},
		{
			"Name":"Phone"
		},
		{
			"Name":"Fax",
			"DataType":"Integer"
		}
	]
}